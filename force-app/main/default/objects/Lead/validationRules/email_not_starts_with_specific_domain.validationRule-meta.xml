<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>email_not_starts_with_specific_domain</fullName>
    <active>false</active>
    <errorConditionFormula>OR(BEGINS(Email, &quot;info@&quot;), BEGINS(Email, &quot;contact@&quot;), BEGINS(Email, &quot;Career@&quot;))</errorConditionFormula>
    <errorMessage>the email starts with info@, contact@, Career@.</errorMessage>
</ValidationRule>
