<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>phone_number</fullName>
    <active>false</active>
    <description>must have 10 digits phone number.</description>
    <errorConditionFormula>AND(LEN( Phone )&lt; 10,  Country = &apos;US&apos;)</errorConditionFormula>
    <errorMessage>must have 10 digits phone number.</errorMessage>
</ValidationRule>
