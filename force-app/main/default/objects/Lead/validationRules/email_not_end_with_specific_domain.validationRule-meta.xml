<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>email_not_end_with_specific_domain</fullName>
    <active>true</active>
    <description>Lead email should not end with .games, .glass, .free, .talk, .garden</description>
    <errorConditionFormula>OR(RIGHT(Email,LEN(Email)-FIND(&apos;.&apos;,Email)+1) = &quot;.games&quot;,RIGHT(Email,LEN(Email)-FIND(&apos;.&apos;,Email)+1) = &quot;.talk&quot;,RIGHT(Email,LEN(Email)-FIND(&apos;.&apos;,Email)+1) = &quot;.free&quot;,RIGHT(Email,LEN(Email)-FIND(&apos;.&apos;,Email)+1) = &quot;.glass&quot;,RIGHT(Email,LEN(Email)-FIND(&apos;.&apos;,Email)+1) = &quot;.garden&quot;)</errorConditionFormula>
    <errorMessage>Lead email should not end with .games, .glass, .free, .talk, .garden</errorMessage>
</ValidationRule>
